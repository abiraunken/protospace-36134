<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to root_path(prototype.title), class: :card__title%>
    <p class="card__summary">
      <%= (prototype.catch_copy) %>
    </p>
    <%= link_to "by",root_path (prototype.name),'#', class: :card__user %>
  </div>
</div>